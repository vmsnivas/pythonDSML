    "def bar(x,y):\n",
    "    if y == 0:\n",
    "        return 0\n",
    "    return (x + bar(x,y - 1))\n",
    "\n",
    "def foo(x,y):\n",
    "    if y == 0:\n",
    "        return 1\n",
    "    return bar(x,foo(x,y - 1))\n",
    "\n",
    "print(foo(3,5))"
